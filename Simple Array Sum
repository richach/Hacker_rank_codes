//Link to teh problem: https://www.hackerrank.com/challenges/simple-array-sum/problem
#include<iostream>
using namespace std;

int simpleArraySum(int arr[], int n)
{
    int sum=0;

    for(int i=0;i<n;i++)
    
        sum+=arr[i];
    
    return sum;
}

int main()
{
    int arr[10],i,n,sum;
    cin>>n;
    for(i=0;i<n;i++)
    {
        cin>>arr[i];
    }
    sum=simpleArraySum(arr,n);
    cout<<sum;
    return 0;
}
